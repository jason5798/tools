<% if(test){ %>
	<%- include extend/header %>
	<meta http-equiv="refresh" content="5;url=http://localhost:3000/update">
	更新時間:<%=new Date() %>


	<table width="800" border="1">
		<tr>
			<td>位址</td>
			<td>資料</td>
			<td>接收時間</td>
		</tr>
		<% if(device){ %>
			<tr>
			<td><%= device.macAddr %></td>
			<td><%= device.data %></td>
			<td><%= device.date %></td>
			</tr>
		<% } %>
	</table>
<% }else{ %>
	<%- include extend/myHeader %>
	<iframe src="http://localhost:3000/ui/#/0" width="100%" height="650" frameborder="1" scrolling="no"></iframe>
<% } %>

<% if(test){ %>
	<%- include extend/footer %>
<% }else{ %>
	<%- include extend/myFooter %>
<% } %>

